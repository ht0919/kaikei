<p id="notice"><%= notice %></p>

<h1>Listing Keihis</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Kamoku</th>
      <th>Tekiyou</th>
      <th>Kingaku</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @keihis.each do |keihi| %>
      <tr>
        <td><%= keihi.date %></td>
        <td><%= keihi.kamoku.name %></td>
        <td><%= keihi.tekiyou %></td>
        <td><%= keihi.kingaku %></td>
        <td><%= link_to 'Show', keihi %></td>
        <td><%= link_to 'Edit', edit_keihi_path(keihi) %></td>
        <td><%= link_to 'Destroy', keihi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Keihi', new_keihi_path %>
